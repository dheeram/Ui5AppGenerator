{
	"_version": "1.8.0",
	"sap.app": {
		"id": "myApps.AppGenerator",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "1.0.0"
		},
		"dataSources": {
			"default": {
				"uri": "/sap/opu/odata/sap/default/",
				"type": "OData",
				"settings": {
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"

		]
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.58.0",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "myApps.AppGenerator.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}, {
				"uri": "extra/fiori.css"
			}],
			"js": [{
				"uri": "extra/fiori.js"
			}, {
				"uri": "controller/BaseController.js"
			}]
		},
		"rootView": {
			"viewName": "myApps.AppGenerator.view.App",
			"type": "XML"
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "myApps.AppGenerator.view",
				"controlAggregation": "pages",
				"controlId": "appFrame",
				"clearControlAggregation": false,
				"bypassed": {
					"target": "NotFound"
				}
			},
			"routes": [{
				"name": "appHome",
				"pattern": "",
				"titleTarget": "",
				"greedy": false,
				"target": ["Home"]
			},{
                "name": "Mockdata",
                "pattern": "Mockdata/{Name}",
                "titleTarget": "",
                "greedy": false,
                "target": [
                    "Mockdata"
                ]
            }],
			"targets": {
				"Home": {
					"viewType": "XML",
					"viewName": "Home"
                },
				"Mockdata": {
					"viewType": "XML",
					"transition": "slide",
					"viewPath": "myApps.AppGenerator.view",
					"viewName": "Mockdata",
					"viewId": "Mockdata",
					"viewLevel": 1
                },
				"NotFound": {
					"viewType": "XML",
					"viewName": "NotFound",
					"transition": "show"
				}
			}
		}
	}
}