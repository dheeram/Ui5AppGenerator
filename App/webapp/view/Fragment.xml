<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core" >
	<Dialog
        id="{{DialogName}}"
		contentHeight=""
		title="{{DialogName}}">
		<content>
			
		</content>
		<buttons>
			<Button
				text="{i18n>Bestaetigen}"
                type="Accept"
				press="onSubmit{{DialogName}}"/>
			<Button
				text="{i18n>Abbrechen}"
				press="onClose{{DialogName}}"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>

